#!/usr/bin/csi -s

(import
  (chicken io)
  (chicken string)
  (srfi 1))

(let ((lst (map
             (lambda (i)
               (string-intersperse (filter string->number (string-split i "=<,> ")) ","))
             (read-lines))))
  (print "N = " (length lst) ";")
  (print "BOTS = [|" (string-intersperse lst "|") "|];"))

; vim: ft=scheme
